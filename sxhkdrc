# KEYBINDZ

F2
	note.sh


F12
	search.sh

super + shift + w
	/home/filip/scripts/write.sh


super + b
	brave-nightly

# Mail

super + shift + m 
	st -e neomutt

# yt-search

super + z
	/home/filip/scripts/yt-search.sh

# launch cheat.sh
super + c 
	cheat.sh
	
# lanuch wttr
super + w 
	weather.sh

# history script

super + shift + h
	history.sh

# launch newsboat

super + r
	st -e newsboat

# launch ncmpcpp

super + m
	st -e ncmpcpp
# launch lf file browser

super + e
	st -e lf

# launch notes

super + n
	st -e nvim -c 'Goyo|set linebreak' /home/filip/vimwiki/index.wiki

# launch calcurse

super + shift + c 
	st -e calcurse

# launch emojis

super + shift + e
	/home/filip/scripts/emoji.sh

## Audio control ##

{XF86AudioMute, XF86AudioRaiseVolume, XF86AudioLowerVolume}
	pulseaudio-ctl {mute, up, down} ; $HOME/scripts/dwmbar/volsig.sh

# Next Prev

super + {bracketright, bracketleft}
	mpc {next, prev}

# Play-Pause

Pause
	mpc toggle

## BACKLIGHT BRIGHTNESS

super + {Home, End}
	xbacklight {-inc 10, -dec 10} 
